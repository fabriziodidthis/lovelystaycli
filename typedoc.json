{
  "$schema": "https://typedoc.org/schema.json",
  "name": "LovelyStay Documentation",
  "entryPoints": [
    "src/",
  ],
  "out": "src/docs",
  "jsDocCompatibility": {
    "inheritDocTag": true,
    "ignoreUnescapedBraces": true
  },
  "tsconfig": "tsconfig.json",
  "includeVersion": true,
  "entryPointStrategy": "expand",
  "cacheBust": true,
  "cleanOutputDir": true,
  "customFooterHtml": "<p>Generated by <a href='https://typedoc.org'>TypeDoc</a> for <a href='https://github.com/fabriziodidthis/lovelystaycli'>LovelystayCLI</a>.</p>",
  "hideGenerator": true,
  "basePath": "./",
  "exclude": [
    ".husky/**",
    "eslint.config.js",
    "**/node_modules/**",
    "**/dist/**",
    "**/test/**",
    "**/tests/**",
    "**/spec/**",
    "**/specs/**",
    "**/types/**",
    "**/typings/**",
    "**/docs/**",
    "**/coverage/**",
    "**/examples/**",
    "**/samples/**",
    "**/templates/**",
    "**/test.ts",
    "**/tests.ts",
    "**/spec.ts",
    "**/specs.ts",
    "**/types.ts",
    "**/typings.ts",
    "**/docs.ts",
    "**/coverage.ts",
    "**/examples.ts",
    "**/samples.ts",
    "**/templates.ts"
  ],
  "compilerOptions": {
    "strictNullChecks": false
  },
  "highlightLanguages": [
    "bash",
    "console",
    "css",
    "html",
    "javascript",
    "json",
    "typescript"
  ]
}